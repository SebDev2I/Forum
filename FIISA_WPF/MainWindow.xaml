<Window x:Class="FIISA_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FIISA_WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="752.526">
    <Grid>
        <Button x:Name="cmdHome" Content="Accueil" HorizontalAlignment="Right" Margin="0,0,661,291.667" VerticalAlignment="Bottom" Width="75" RenderTransformOrigin="0.07,-0.217"/>
        <Button x:Name="cmdLogin" Content="Se connecter" Margin="618,9,10,291.667"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="499,9,0,291.667" MinWidth="100" Width="100" MaxWidth="100"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Right" Margin="0,9,267,291.667" TextWrapping="Wrap" Text="TextBox" Width="120"/>
        <TextBlock x:Name="textBlock" Text="{Binding NameUser}" HorizontalAlignment="Left" Margin="182,90,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock1" Text="{Binding FirstnameUser}" HorizontalAlignment="Left" Margin="229,145,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>


    </Grid>
</Window>
